{
  "cli": {
    "version": ">= 5.6.0"
  },
  "build": {
    "base": {
      "env": {
        "EXAMPLE_ENV": "example value"
      },
      "android": {
        "image": "default",
        "env": {
          "PLATFORM": "android"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        }
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "BASEURL": "https://skit.st1.2345.cn"
        }
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "env": {
          "BASEURL": "https://skit.contentchina.com"
        },
        "buildType": "apk"
      }
    },
    "production": {
      "extends": "base",
      "android": {
        "env": {
          "BASEURL": "https://skit.contentchina.com"
        }
      },
      "ios": {}
    }
  },
  "submit": {
    "production": {}
  }
}
